@model IEnumerable<WebApplication3.Work_Data>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Shift_Date_End)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Shift_Time_End)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Worker.Name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Shift_Date_End)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Shift_Time_End)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Worker.Name)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ID })
        </td>
    </tr>
}

</table>
